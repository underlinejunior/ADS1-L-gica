"Crie uma função Media(), que recebe 2 notas de um aluno e retornar a sua média
para o programa principal."
	funcao cadeia Media(real a,real b){
		cadeia resultado = "A media entre "+a+" e "+b+" é "+ ((a+b)/2)
			retorne resultado
		}

	funcao inicio(){
		real a, b
		escreva("digite a primeira nota: ")
		leia(a)
		escreva("digite a segunda nota: ")
		leia(b)
        Media(a,b)
	}